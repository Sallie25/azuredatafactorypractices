{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "deeptechdatafactory"
		},
		"deeptech_AzureBlobStorage_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'deeptech_AzureBlobStorage_LS'"
		},
		"deeptech_AzureSqlDatabase_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'deeptech_AzureSqlDatabase_LS'"
		},
		"API_RestServices_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/posts"
		},
		"deeptech_AzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "deeptech-salome-sql-server.database.windows.net"
		},
		"deeptech_AzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "deeptech-sql-db"
		},
		"deeptech_AzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "deeptech-salome"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "deeptechdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"deeptech_AzureBlobStorage_LS_connectionString": {
						"value": "[parameters('deeptech_AzureBlobStorage_LS_connectionString')]"
					},
					"deeptech_AzureSqlDatabase_LS_password": {
						"value": "[parameters('deeptech_AzureSqlDatabase_LS_password')]"
					},
					"API_RestServices_LS_properties_typeProperties_url": {
						"value": "[parameters('API_RestServices_LS_properties_typeProperties_url')]"
					},
					"deeptech_AzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('deeptech_AzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"deeptech_AzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('deeptech_AzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"deeptech_AzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('deeptech_AzureSqlDatabase_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "deeptechdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'deeptechdatafactory_ArmTemplate_0')]"
			]
		}
	]
}